{
    "defaultSeverity": "error",
    "extends": [
        "tslint-config-airbnb"
    ],
    "jsRules": {},
    "rules": {
		"import-name": false,
		"max-line-length": false,
		"indent": [true, "tabs"],
		"ter-indent": [true, "tab"],
		"trailing-comma": [true, "never"],
		"no-parameter-reassignment": false
	},
    "rulesDirectory": []
}
